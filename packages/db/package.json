{
  "name": "@duramation/db",
  "version": "1.0.0",
  "description": "the database package for project duramation",
  "author": "abdessamie.elmoubarki@gmail.com",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn db:generate && rimraf dist && tsc && cp -R src/generated dist",
    "dev": "tsc --watch",
    "db:generate": "prisma generate --no-engine",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio"
  },
  "exports": {
    ".": "./dist/index.js",
    "./types": "./dist/types.d.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@prisma/extension-accelerate": "^2.0.2",
    "prisma-field-encryption": "^1.6.0"
  },
  "devDependencies": {
    "@duramation/typescript-config": "*",
    "@types/node": "^24.5.2",
    "prisma": "6.17.1",
    "typescript": "^5.9.2"
  }
}
