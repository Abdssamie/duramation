{
  "name": "@duramation/db",
  "version": "1.0.0",
  "description": "the database package for project duramation",
  "author": "abdessamie.elmoubarki@gmail.com",
  "license": "MIT",
  "private": true,
  "files": [
    "dist",
    "schema.prisma"
  ],
  "scripts": {
    "build": "pnpm db:generate && tsc --incremental false",
    "dev": "tsc --watch",
    "clean": "rimraf dist .turbo",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": "./dist/index.js",
    "./types": "./dist/types.js"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@prisma/extension-accelerate": "^2.0.2",
    "prisma-field-encryption": "^1.6.0"
  },
  "devDependencies": {
    "@duramation/typescript-config": "workspace:*",
    "@types/node": "^24.5.2",
    "prisma": "6.17.1",
    "typescript": "^5.9.2"
  }
}